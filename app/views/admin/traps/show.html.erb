<div>
  <h1 class="admin-h1"><%=  @trap.name %></h1>
</div>

<div class='margin-top-10' >
  <div>Challenge rating - <%= @trap.challenge_rating %> </div>
  <div>Experience - <%= @trap.experience %> </div>
  <div>Type of system - <%= @trap.type_of_system.humanize %> </div>
  <div>Reset - <%= @trap.reset %> </div>
  <div>Trigger - <%= @trap.trigger %> </div>
  <div>Perception - <%= @trap.perception %> </div>
  <div>Disable Device - <%= @trap.disable_devise %> </div>
  <div>Effect - <%= @trap.effect %> </div>
  <% if @trap.description.present? %>
    <div>Description - <%= @trap.description %> </div>
  <% end %>
  <br>
  <%= link_to 'Back', admin_traps_path, :class => 'btn btn-primary' %>
</div>

