<div>
  <h1>Users</h1>
</div>


<div class='margin-top-10' >
  <table class='table'>
    <thead>
      <tr>
        <th>ID</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <%= paginate @users %>
</div>
